package raahat;

/**
 *
 * @author Raahat rajan
 */
public class Raahat {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
    
    // Traffic lights 
    
    int r = 5;
    int o = 8;
    int g = 11;
    String green = "go";
    String red = "stop";
    String orange = "wait";
    
    if(r==5){
        System.out.println(red); 
    }else{
        System.out.println("wrong script");}
    if(o==8){
        System.out.println(orange);
    }else{
         System.out.println("wrong script");}
    if(g==11){
        System.out.println(green);
    }else{
        System.out.println("wrong script");}
    } 
}
    
    
    
            
       
    
    
    

